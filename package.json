{
  "bin": {
    "spriter": "bin/spriter.js"
  },
  "name": "calcite-ui-icons",
  "version": "1.0.0",
  "private": false,
  "description": "A collection of UI SVG icons created by Esri for applications.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/calcite-ui-icons.git"
  },
  "keywords": [
    "esri",
    "arcgis",
    "apps",
    "icons",
    "svg"
  ],
  "contributors": [
    {
      "name": "Julio Ochoa",
      "email": "<jochoa@esri.com>"
    },
    {
      "name": "Paul Pederson ",
      "email": "<PPederson@esri.com>"
    },
    {
      "name": "John Griffith",
      "email": "<jgriffith@esri.com>"
    },
    {
      "name": "Katelyn Seitz",
      "email": "<kseitz@esri.com>"
    }
  ],
  "license": "SEE LICENSE IN README.md",
  "bugs": {
    "url": "https://github.com/Esri/calcite-ui-icons/issues"
  },
  "homepage": "https://github.com/Esri/calcite-ui-icons#readme",
  "scripts": {
    "spriter": "node bin/spriter.js",
    "imageminsvgo": "node bin/imagemin-svgo.js",
    "buildsprite-16": "svgstore -o sprite-16.svg icons/*-16.svg",
    "buildsprite-24": "svgstore -o sprite-24.svg icons/*-24.svg",
    "buildsprite-32": "svgstore -o sprite-32.svg icons/*-32.svg",
    "buildsprite-16-f": "svgstore -o sprite-16-f.svg icons/*-16-f.svg",
    "buildsprite-24-f": "svgstore -o sprite-24-f.svg icons/*-24-f.svg",
    "buildsprite-32-f": "svgstore -o sprite-32-f.svg icons/*-32-f.svg",
    "optimize": "npm run imageminsvgo",
    "start": "npm run buildsprite-16-f && npm run buildsprite-24-f && npm run buildsprite-32-f && npm run buildsprite-16 && npm run buildsprite-24 && npm run buildsprite-32 && npm run optimize"
  },
  "dependencies": {
    "promise": "~8.0.1"
  },
  "devDependencies": {
    "imagemin": "^5.3.1",
    "imagemin-svgo": "^6.0.0",
    "svgstore-cli": "^1.3.1"
  }
}
